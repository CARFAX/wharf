html, body
    height 100%

pre
   margin-top 0

code, blockquote
    font-size 16px
    background #f2f2f2
    padding 2px 4px
    border-radius 3px
    line-height 1.4
    color #000
    font-family consolas, menlo, monaco, monospace

blockquote
    display block
    margin 0

    p
        margin 0
        font-family consolas, menlo, monaco, monospace

pre code
    display block
    padding 0.5em
    background #f2f2f2


.header-container

    .object-header
        margin-top 1.5em
        color #007fb2

    .object-header + p
        margin-bottom 2em

    .member-header
        margin-top 1.5em

    .btn
        &.test, &.src
            float right
            font-size 12px
            margin-left .5em
    &.h2
        .btn
            &.test, &.src
                margin-top 11px
    &.h3
        .btn
            &.test, &.src
                margin-top 5px